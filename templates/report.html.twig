{# templates/lucky_number.html.twig #}

{% extends "base.html.twig" %}

{% block title %}Report{% endblock %}

{% block main %}
    <h1>Report</h1>
    <div class="container">
        <div class="column-1">
            {% for kmom in ['kmom01', 'kmom02', 'kmom03', 'kmom04', 'kmom05', 'kmom06', 'kmom0710'] %}
                {{ include('report/' ~ kmom ~ '.markdown.twig')|markdown_to_html }}
            {% endfor %}
        </div>
        <div class="column-2">
            <h2>Kursmoment</h2>
            {% for kmom in ['kmom01', 'kmom02', 'kmom03', 'kmom04', 'kmom05', 'kmom06'] %}
                <p><a href="{{ path('report') }}#{{ kmom }}">{{ kmom|capitalize }}</a></p>
            {% endfor %}
            <p><a href="{{ path('report') }}#kmom0710">Kmom07/10</a></p>
        </div>
    </div>
    

{% endblock %}